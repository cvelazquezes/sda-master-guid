{
  "name": "sda-master-guid",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "lint:report": "eslint . --ext .ts,.tsx --format html --output-file reports/eslint-report.html",
    "lint:cache": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.cache/eslint",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "validate": "npm run type-check && npm run lint:strict && npm run test:ci",
    "validate:quick": "npm run type-check && npm run lint:cache",
    "build": "tsc && expo export",
    "version:patch": "node scripts/version-bump.js patch",
    "version:minor": "node scripts/version-bump.js minor",
    "version:major": "node scripts/version-bump.js major",
    "version:prerelease": "node scripts/version-bump.js prerelease",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test": "detox test",
    "e2e:test:record": "detox test --record-videos all",
    "storybook": "sb-rn-get-stories && expo start --port 7007",
    "storybook:generate": "sb-rn-get-stories"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@sentry/react-native": "~6.10.0",
    "@shopify/flash-list": "1.7.3",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.6.5",
    "date-fns": "^3.0.0",
    "expo": "^52.0.0",
    "expo-calendar": "~14.0.6",
    "expo-constants": "~17.0.8",
    "expo-linear-gradient": "~14.0.2",
    "expo-secure-store": "~14.0.1",
    "expo-status-bar": "~2.0.1",
    "i18next": "^25.7.1",
    "immer": "^10.0.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^16.3.5",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.10",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@storybook/addon-ondevice-actions": "^10.0.7",
    "@storybook/addon-ondevice-controls": "^10.0.7",
    "@storybook/react-native": "^10.0.7",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "detox": "^20.13.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-summary": "^2.0.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "jest-junit": "^16.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix --cache --cache-location node_modules/.cache/eslint",
      "prettier --write"
    ]
  }
}
